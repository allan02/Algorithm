    "def solution(sizes):\n",
    "    answer = 0\n",
    "    temp = []\n",
    "    for i in sizes:\n",
    "        temp.append(sorted(i))\n",
    "    \n",
    "    x = 0\n",
    "    y = 0\n",
    "    \n",
    "    for j in temp:\n",
    "        if j[0] > x:\n",
    "            x = j[0]\n",
    "        if j[1] > y:\n",
    "            y = j[1]\n",
    "            \n",
    "    answer = x * y\n",
    "    return answer"
   ]
  }
