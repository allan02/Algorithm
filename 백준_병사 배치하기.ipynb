   ],
   "source": [
    "n = int(input())\n",
    "\n",
    "array = list(map(int,input().split()))\n",
    "\n",
    "dp = [1] * n\n",
    "\n",
    "for i in range(1, n):\n",
    "    for j in range(i):\n",
    "        if array[i] < array[j]:\n",
    "            dp[i] = max(dp[i], dp[j] + 1)\n",
    "            \n",
    "print(n - max(dp))"
   ]
