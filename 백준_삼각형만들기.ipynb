    "n = int(input())\n",
    "answer = 0\n",
    "for a in range(1, n):\n",
    "    for b in range(a, n):\n",
    "        c = n - a - b\n",
    "        if c >= b:\n",
    "            if c < b + a:\n",
    "                answer += 1\n",
    "                \n",
    "        else:\n",
    "            break\n",
    "            \n",
    "print(answer)"
   ]
  }
