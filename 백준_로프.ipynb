   "source": [
    "n = int(input())\n",
    "array = []\n",
    "answer = 0\n",
    "\n",
    "for _ in range(n):\n",
    "    array.append(int(input()))\n",
    "    \n",
    "array.sort(reverse = True)\n",
    "\n",
    "while array:\n",
    "    l = len(array)\n",
    "    answer = max(answer, array[l - 1] * l)\n",
    "    array.pop()\n",
    "    \n",
    "print(answer)"
   ]
  }
