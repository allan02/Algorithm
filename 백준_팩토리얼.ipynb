    "def fac(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    \n",
    "    return fac(n - 1) * n\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "print(fac(n))\n"
   ]
