    "def solution(citations):\n",
    "    answer = 0\n",
    "    \n",
    "    for i in citations:\n",
    "        count = 0\n",
    "        for j in citations:\n",
    "            if j >= i:\n",
    "                count += 1\n",
    "        if count <= i:\n",
    "            answer = max(answer, count)\n",
    "    \n",
    "    return answer"
   ]
  }
