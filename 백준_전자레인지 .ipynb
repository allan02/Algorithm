    "t = int(input())\n",
    "a = 0\n",
    "b = 0\n",
    "c = 0\n",
    "\n",
    "if t >= 300:\n",
    "    a += t // 300\n",
    "    t = t % 300\n",
    "    \n",
    "if t >= 60:\n",
    "    b += t // 60\n",
    "    t = t % 60\n",
    "    \n",
    "if t >= 10:\n",
    "    c += t // 10\n",
    "    t = t % 10\n",
    "    \n",
    "if t == 0:\n",
    "    print(a, b, c)\n",
    "    \n",
    "else:\n",
    "    print(-1)\n",
    "\n"
   ]
