    "def solution(absolutes, signs):\n",
    "    answer = 0\n",
    "    for x, y in zip(absolutes, signs):\n",
    "        if y == True:\n",
    "            answer += x\n",
    "        else:\n",
    "            answer -= x\n",
    "    return answer"
   ]
