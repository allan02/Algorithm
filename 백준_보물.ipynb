    "array = list(map(int, input().split()))\n",
    "array2 = list(map(int, input().split()))\n",
    "\n",
    "array = sorted(array, key = lambda x : -x)\n",
    "array2 = sorted(array2)\n",
    "\n",
    "answer = 0\n",
    "\n",
    "for i in range(n):\n",
    "    answer += array[i] * array2[i]\n",
    "\n",
    "print(answer)"
